{
	"name": "youtube-download",
	"author": "Wixonic <contact@wixonic.fr> (https://wixonic.fr)",
	"bugs": {
		"url": "https://github.com/Wixonic/YouTube-download/issues",
		"email": "contact@wixonic.fr"
	},
	"homepage": "https://github.com/Wixonic/YouTube-download#readme",
	"license": "MIT",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Wixonic/YouTube-download.git"
	},
	"main": "app.js",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make"
	},
	"engines": {
		"node": ">=16.x.x"
	},
	"dependencies": {
		"ytdl-core": "^4.11.4"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.1.1",
		"@electron-forge/maker-dmg": "^6.1.1",
		"electron": "^24.4.0"
	},
	"config": {
		"forge": {
			"packagerConfig": {
				"appCopyright": "Copyright (c) 2023 Wixonic",
				"icon": "./assets/icon",
				"overwrite": true,
				"name": "YouTube Download"
			},
			"makers": [
				{
					"name": "@electron-forge/maker-dmg",
					"config": {
						"format": "ULFO",
						"icon": "./assets/icon.png",
						"name": "YouTube Download"
					}
				}
			]
		}
	}
}